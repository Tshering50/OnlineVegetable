   <div class="container">
      <form action="/register" method="post">
          <div class="d-flex justify-content-center">   
            <div class=" col-xl-6">
                      <form class="form-group" action="/register" method="POST">
                          <div class="form-outline my-4">
                              <input type="text" id="name" class="form-control form-control-lg"
                                  autocomplete="off" style="height: 50px;" name="name" Required/>
                              <label class="form-label" for="form2Example17">Name</label>
                          </div>
          
                          <div class="form-outline mb-4">
                              <input type="email" id="email"
                                  class="form-control form-control-lg" style="height: 50px;" name="email" Required/>
                              <label class="form-label" for="form2Example27">Email</label>
                          </div>
                          <div class="form-outline my-4">
                            <input type="text" id="address" class="form-control form-control-lg"
                                autocomplete="off" style="height: 50px;" name="address" Required/>
                            <label class="form-label" for="form2Example17">Address</label>
                        </div>
                        <div class="form-outline my-4">
                            <input type="tel" id="phone" class="form-control form-control-lg"
                                autocomplete="off" style="height: 50px;" name="phoneno" Required/>
                            <label class="form-label" for="form2Example17">Phone Number</label>
                        </div>
                        <div class="form-outline my-4">
                            <input type="password" id="password" class="form-control form-control-lg"
                                autocomplete="off" style="height: 50px;" name="password" Required/>
                            <label class="form-label" for="form2Example17">Password</label>
                        </div>
                        <div class="form-outline my-4">
                            <input type="password" id="confirmpassword" class="form-control form-control-lg"
                                autocomplete="off" style="height: 50px;" name="confirmpassword" Required/>
                            <label class="form-label" for="form2Example17">Confirm Password</label>
                        </div>
          
          
                          <div class="pt-1 mb-4">
                              <button class="btn btn-lg btn-block" type="submit" id="Usersubmit"
                                  style="background: #0b8183; color:#fff; font-size:12pt; height: 50px;">Register</button>
                          </div>
          
                          <!-- <a class="" style="color: rgb(231, 71, 71); align-items: center;" href="./forgetpassword">Forgot password?</a>    -->
                      </form> 
                      <div class="pt-4 text-center">
                        Have an Account?. <a href="/login">Sign in</a>
                      </div>
                      
                  </div>
          
          </div>
      
